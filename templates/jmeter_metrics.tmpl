<div class="panel panel-default">
    <div clas="panel-header">
        <span class="label label-primary">Jmeter测试报告</label>
    </div>
    <div class="panel-body">
        <table class="table table-striped table-bordered">
            <tbody>
                <tr>
                    <td>测试计划</td>
                    <td>{{ .log.JobName }}</td>
                </tr>
                <tr>
                    <td>开始时间</td>
                    <td>{{ .log.StartTs|strftime }}</td>
                </tr>
                <tr>
                    <td>结束时间</td>
                    <td>{{ .log.EndTs|strftime }}</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

<div class="panel panel-default">
    <div clas="panel-header">
        <span class="label label-primary">列表报告</label>
    </div>
    <div class="panel-body">
        <table class="table table-striped">
            <tr>
                <th>HTTP请求</th>
                <th>请求总数</th>
                <th>平均响应时间</th>
                <th>响应时间[Median]</th>
                <th>响应时间[P90]</th>
                <th>响应时间[P95]</th>
                <th>响应时间[P99]</th>
                <th>Min</th>
                <th>Max</th>
                <th>错误率%</th>
                <th>QPS</th>
                <th>KB/sec</th>
            </tr>
            {{ range .log.MetricsList }}
            <tr>
                <td>{{ .Label }}</td>
                <td>{{ .Total }}</td>
                <td>{{ .Latencies.Average }} ms</td>
                <td>{{ .Latencies.P50 }} ms</td>
                <td>{{ .Latencies.P90 }} ms</td>
                <td>{{ .Latencies.P95 }} ms</td>
                <td>{{ .Latencies.P99 }} ms</td>
                <td>{{ .Latencies.Min }} ms</td>
                <td>{{ .Latencies.Max }} ms</td>
                <td>{{ .ErrorRate }}%</td>
                <td>{{ .Qps }}</td>
                <td>{{ .Kbs }}</td>
            </tr>
            {{ end }}
        </table>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel-header">
        <span class="label label-primary">图形报告</label>
    </div>
    <div class="panel-body">
        <div class="row">
            <div class="col-md-12">
                <div id="graph_tps" style="width:100%;height:500px"></div>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
  var graph_tps = echarts.init(document.getElementById("graph_tps"))
  var TpsMetrics = {{ .log.TransactionMetrics }}
  var xElapsed = TpsMetrics.elapsed;
  var dSeries  = TpsMetrics.series;
  var dataList = TpsMetrics.data_list;
  var dySeries = []
  for (var i = 0; i < dSeries.length; i++) {
      dySeries.push(dSeries[i]+" (success)")
      dySeries.push(dSeries[i]+" (failure)")
  }
  for (var i = 0; i < dataList.length; i++) {
      var j = 0;
      for (j = 0; j < dataList[i].success.length; j++) {
          if (dataList[i].success[j] == -1) {
              dataList[i].success[j] = '-'
          }
      }
      for (j = 0; j < dataList[i].failure.length; j++) {
          if (dataList[i].failure[j] == -1) {
              dataList[i].failure[j] = '-'
          }
      }
   }

   var series = [];
   for (var i = 0; i < dataList.length; i++) {
       series.push({
           connectNulls: true,
  name: dSeries[i]+" (success)",
  type:'line',
  data: dataList[i].success,
  markPoint: {
  data: [
  {type: 'max', name: '最大值'},
  {type: 'min', name: '最小值'}
  ]
  },
  markLine: {
  data: [
  {type: 'average', name: '平均值'}
  ]
  }
  });

  series.push({
      connectNulls: true,
      name: dSeries[i] + " (failure)",
  type:'line',
  data: dataList[i].failure,
  markPoint: {
  data: [
  {name: '周最低', value: -2, xAxis: 1, yAxis: -1.5}
  ]
  },
  markLine: {
  data: [
  {type: 'average', name: '平均值'},
  [{
  symbol: 'none',
  x: '90%',
  yAxis: 'max'
  }, {
  symbol: 'circle',
  label: {
  normal: {
  position: 'start',
  formatter: '最大值'
  }
  },
  type: 'max',
  name: '最高点'
  }]
  ]
  }
  });
   }

  var option = {
  title: {
  text: 'TPS分布图',
  subtext: {{ .log.JobName }}
  },
  tooltip: {
  trigger: 'axis'
  },
  legend: {
                       data: dySeries,
                       left: '10%'
  },
  toolbox: {
  show: true,
  feature: {
  dataZoom: {
  yAxisIndex: 'none'
  },
  dataView: {readOnly: false},
  magicType: {type: ['line', 'bar']},
  restore: {},
  saveAsImage: {}
  }
  },
  xAxis:  {
  type: 'category',
  boundaryGap: false,
  data: xElapsed
  },
  yAxis: {
  type: 'value',
  axisLabel: {
  formatter: '{value}'
  }
  },
  series: series
  };
  graph_tps.setOption(option)

</script>
<script type="text/javascript">
$(function () {
      $('[data-toggle="popover"]').popover()
})

$(window).resize(function(){
  graph_tps.resize();
})
</script>
